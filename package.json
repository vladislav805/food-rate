{
  "name": "@veluga/food-rate",
  "version": "0.0.0",
  "description": "Food rate",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "npm-run-all -p dev:frontend dev:backend dev:client",
    "dev:backend": "ts-node -r tsconfig-paths/register src/server",
    "dev:frontend": "ts-node-dev --require ./ts-loader.fix.js -r tsconfig-paths/register src/frontend/main@server.tsx",
    "dev:client": "webpack serve",
    "test": "ts-node -r tsconfig-paths/register src/test",
    "clean": "rm -rf dist",
    "build": "npm-run-all -p build:frontend build:server",
    "build:server": "webpack --config ./webpack.server.config.js",
    "build:frontend": "NODE_ENV=production webpack --config ./webpack.config.js"
  },
  "keywords": [
    "food"
  ],
  "author": "Vladislav Veluga <vlad@velu.ga> (https://velu.ga/)",
  "license": "ISC",
  "dependencies": {
    "@bem-react/classname": "1.5.12",
    "cookie-parser": "1.4.6",
    "dotenv": "16.0.0",
    "express": "4.17.2",
    "mysql2": "2.3.3",
    "react": "18.0.0-rc.0",
    "react-dom": "18.0.0-rc.0",
    "react-router": "6.2.1",
    "react-router-dom": "6.2.1",
    "sequelize": "6.14.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.13",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/validator": "13.7.1",
    "css-loader": "6.6.0",
    "mini-css-extract-plugin": "2.5.3",
    "npm-run-all": "4.1.5",
    "null-loader": "4.0.1",
    "sass": "1.49.7",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "3.12.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.5.5",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  }
}
